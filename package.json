{
  "name": "tohteng",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "yarn": "1.16.0",
    "node": "12.5.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "antd": "^3.23.4",
    "apollo": "^2.19.1",
    "apollo-boost": "^0.4.4",
    "babel-plugin-import": "^1.12.2",
    "customize-cra": "^0.7.0",
    "graphql": "^14.5.8",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-router": "^5.1.2",
    "react-scripts": "3.1.2",
    "typescript": "3.6.3"
  },
  "scripts": {
    "postinstall": "yarn load_schema",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "download_schema": "npx apollo schema:download --endpoint=https://angjigao.herokuapp.com/ schema.json",
    "codegen": "npx apollo codegen:generate --globalTypesFile=globalTypes.ts --localSchemaFile=schema.json --target=typescript --includes=src/**/*.gql --tagName=gql --addTypename --outputFlat src/ApolloTypes",
    "load_schema": "yarn download_schema && yarn codegen"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react-hooks": "^2.0.1",
    "prettier": "1.18.2"
  }
}
